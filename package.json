{
  "name": "r-film",
  "private": true,
  "version": "2.6.4",
  "type": "module",
  "main": "electron/main.js",
  "build": {
    "appId": "com.ronial.rfilm",
    "icon": "public/filmlogov3_round.ico",
    "files": [
      "dist/**/*",
      "electron/**/*"
    ],
    "directories": {
      "buildResources": "public",
      "output": "win"
    },
    "win": {
      "target": "nsis",
      "icon": "public/filmlogov3_round.ico"
    },
    "mac": false,
    "linux": false
  },
  "scripts": {
    "dev": "vite",
    "dev:electron": "vite && electron .\"",
    "lint": "eslint .",
    "preview": "vite preview",
    "build": "tsc -b && vite build",
    "build:cap": "tsc -b && vite build & npx cap sync",
    "build:electron": "vite build && electron-builder",
    "start": "node electron/start.js",
    "start:electron": "concurrently \"npx http-server ./dist -p 2004\" \"electron .\""
  },
  "dependencies": {
    "@awesome-cordova-plugins/file-opener": "^6.16.0",
    "@capacitor-community/http": "^1.4.1",
    "@capacitor/android": "^7.2.0",
    "@capacitor/app": "^7.0.1",
    "@capacitor/browser": "^7.0.1",
    "@capacitor/cli": "^7.2.0",
    "@capacitor/clipboard": "^7.0.1",
    "@capacitor/core": "^7.2.0",
    "@capacitor/device": "^7.0.1",
    "@capacitor/filesystem": "^7.0.1",
    "@capacitor/ios": "^7.2.0",
    "@capacitor/preferences": "^7.0.1",
    "@capacitor/share": "^7.0.1",
    "@ionic-native/app-version": "^5.36.0",
    "@radix-ui/react-avatar": "^1.1.7",
    "@radix-ui/react-dropdown-menu": "^2.1.12",
    "@radix-ui/react-hover-card": "^1.1.11",
    "@radix-ui/react-select": "^2.2.2",
    "@radix-ui/react-separator": "^1.1.4",
    "@radix-ui/react-slot": "^1.2.0",
    "@tailwindcss/vite": "^4.1.4",
    "aos": "^2.3.4",
    "axios": "^1.9.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cordova-plugin-app-version": "^0.1.14",
    "cordova-plugin-file-opener2": "^4.0.0",
    "firebase": "^11.6.1",
    "framer-motion": "^12.9.2",
    "localforage": "^1.10.0",
    "lucide-react": "^0.503.0",
    "match-sorter": "^8.0.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-helmet": "^6.1.0",
    "react-hot-toast": "^2.5.2",
    "react-router-dom": "^7.5.2",
    "react-slick": "^0.30.3",
    "slick-carousel": "^1.8.1",
    "sort-by": "^1.2.0",
    "tailwind-merge": "^3.2.0",
    "tailwindcss": "^4.1.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.22.0",
    "@types/aos": "^3.0.7",
    "@types/node": "^22.15.2",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@types/react-helmet": "^6.1.11",
    "@types/react-slick": "^0.23.13",
    "@vitejs/plugin-react-swc": "^3.8.0",
    "7zip-bin": "^5.2.0",
    "concurrently": "^9.1.2",
    "electron": "^36.0.0",
    "electron-builder": "^26.0.12",
    "eslint": "^9.22.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "pm2": "^6.0.5",
    "tw-animate-css": "^1.2.8",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.26.1",
    "vite": "^6.3.1"
  }
}
